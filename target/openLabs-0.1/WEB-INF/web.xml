<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>TestServlet</servlet-name>
        <servlet-class>com.polymorphic.openlabs.TestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LabDetailServlet</servlet-name>
        <servlet-class>com.polymorphic.openlabs.LabDetailServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.polymorphic.openlabs.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginPage</servlet-name>
        <servlet-class>com.polymorphic.openlabs.loginPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Da201</servlet-name>
        <servlet-class>com.polymorphic.openlabs.Da201</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestServlet</servlet-name>
        <url-pattern>/TestServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LabDetailServlet</servlet-name>
        <url-pattern>/LabDetailServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginPage</servlet-name>
        <url-pattern>/loginPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Da201</servlet-name>
        <url-pattern>/Da201</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
